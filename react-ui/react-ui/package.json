{
  "name": "react-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "collect": "react-scripts build && npm run build-rename && npm run copy-buildfiles && npm run collectstatic",
    "build-rename": "npm run build-rename-js && npm run build-rename-css && npm run build-rename-logo",
    "build-rename-logo": "renamer --find '/([^\\.])\\.[^\\.]+\\.(\\w+)/' --replace '$1.$2' build/static/media/*",
    "build-rename-js": "renamer  --find '/([^\\.]+)\\.[0-9A-z]+\\.([\\w\\.]+)/' --replace '$1.$2' build/static/js/*",
    "build-rename-css": "renamer --find '/([^\\.]+)\\.[0-9A-z]+\\.([\\w\\.]+)/' --replace '$1.$2' build/static/css/*",
    "copy-buildfiles": "npm run copy-build-js && npm run copy-build-css && npm run copy-build-logo",
    "copy-build-logo": "copyfiles -f 'build/static/media/*' '../../static/img/'",
    "copy-build-js": "copyfiles -f 'build/static/js/*' '../../static/js/'",
    "copy-build-css": "copyfiles -f 'build/static/css/*' '../../static/css/'",
    "collectstatic": "python ../../manage.py collectstatic --no-input"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "react-scripts": "^2.1.3",
    "renamer": "^1.1.1"
  }
}
